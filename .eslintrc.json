{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "no-else-return": 0,
    "no-console": 0,
    "react/jsx-no-bind": 0,
    "key-spacing": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-restricted-syntax": 0
  },
  "parser": "babel-eslint"
}
